					"dataset": {
						"referenceName": "ParquetDataSet",
						"type": "DatasetReference"
					},
					"name": "Equipment"
				},
				{
					"dataset": {
						"referenceName": "ParquetDataSet",
						"type": "DatasetReference"
					},
					"name": "Area"
				},
				{
					"dataset": {
						"referenceName": "ParquetDataSet",
						"type": "DatasetReference"
					},
					"name": "StandardTable"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "EquipmentAndArea"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> Equipment",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> Area",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> StandardTable",
				"Area, StandardTable join(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> EquipmentAndArea"
			]
		}
	}
