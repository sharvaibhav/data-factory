{
	"name": "TestingDataFlow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "mysqlEquipment"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          PartitionKey as string,",
				"          RowKey as string,",
				"          Timestamp as string,",
				"          type as string,",
				"          tagNumber as string,",
				"          description as string,",
				"          relations as string,",
				"          facility as string,",
				"          area as string,",
				"          system as string,",
				"          discipline as string,",
				"          procurementPackage as string,",
				"          processLine as string,",
				"          id as string,",
				"          topTag as string,",
				"          attributes as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     limit: 10) ~> mysqlEquipment",
				"mysqlEquipment sink(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          PartitionKey,",
				"          RowKey,",
				"          Timestamp,",
				"          type,",
				"          tagNumber,",
				"          description,",
				"          relations,",
				"          facility,",
				"          area,",
				"          system,",
				"          discipline,",
				"          procurementPackage,",
				"          processLine,",
				"          id,",
				"          topTag,",
				"          attributes",
				"     ),",
				"     format: 'table') ~> sink1"
			]
		}
	}
}