{
	"name": "TestingDataFlow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "MYSQLEquipment",
						"type": "DatasetReference"
					},
					"name": "mysqlEquipment"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "MySqlEquipmentSink",
						"type": "DatasetReference"
					},
					"name": "mysqlEquipmenrSink"
				}
			],
			"transformations": [
				{
					"name": "streamOne"
				}
			],
			"scriptLines": [
				"source(output(",
				"          PartitionKey as string,",
				"          RowKey as string,",
				"          Timestamp as string,",
				"          type as string,",
				"          tagNumber as string,",
				"          description as string,",
				"          relations as string,",
				"          facility as string,",
				"          area as string,",
				"          system as string,",
				"          discipline as string,",
				"          procurementPackage as string,",
				"          processLine as string,",
				"          id as string,",
				"          topTag as string,",
				"          attributes as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     batchSize: 3,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> mysqlEquipment",
				"mysqlEquipment alterRow(updateIf(id==\"Johan Castberg_JC_A3RZ75325T\"),",
				"     deleteIf(id==\"Johan Castberg_JC_A3RZ7YU3UD\"),",
				"     upsertIf(id==\"Johan Castberg_JC_A3RZ7SYVUD\")) ~> streamOne",
				"streamOne sink(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     input(",
				"          PartitionKey as string,",
				"          RowKey as string,",
				"          Timestamp as string,",
				"          type as string,",
				"          tagNumber as string,",
				"          description as string,",
				"          relations as string,",
				"          facility as string,",
				"          area as string,",
				"          system as string,",
				"          discipline as string,",
				"          procurementPackage as string,",
				"          processLine as string,",
				"          id as string,",
				"          topTag as string,",
				"          attributes as any",
				"     ),",
				"     deletable:true,",
				"     insertable:true,",
				"     updateable:true,",
				"     upsertable:true,",
				"     keys:['PartitionKey','id'],",
				"     skipKeyWrites:true,",
				"     format: 'table',",
				"     mapColumn(",
				"          PartitionKey,",
				"          RowKey,",
				"          type,",
				"          tagNumber,",
				"          id",
				"     )) ~> mysqlEquipmenrSink"
			]
		}
	}
}