{
	"name": "TestingDataFlow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "MYSQLEquipment",
						"type": "DatasetReference"
					},
					"name": "mysqlEquipment"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "MySqlEquipmentSink",
						"type": "DatasetReference"
					},
					"name": "mysqlEquipmenrSink"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          PartitionKey as string,",
				"          RowKey as string,",
				"          Timestamp as string,",
				"          area as string,",
				"          attributes as string,",
				"          description as string,",
				"          discipline as string,",
				"          facility as string,",
				"          id as string,",
				"          processLine as string,",
				"          procurementPackage as string,",
				"          system as string,",
				"          tagNumber as string,",
				"          topTag as string,",
				"          type as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     batchSize: 3,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> mysqlEquipment",
				"mysqlEquipment sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          PartitionKey as string,",
				"          RowKey as string,",
				"          type as string,",
				"          tagNumber as string,",
				"          description as string,",
				"          relations as string,",
				"          facility as string,",
				"          area as string,",
				"          system as string,",
				"          discipline as string,",
				"          procurementPackage as string,",
				"          processLine as string,",
				"          id as string,",
				"          topTag as string,",
				"          attributes as any",
				"     ),",
				"     deletable:true,",
				"     insertable:true,",
				"     updateable:true,",
				"     upsertable:true,",
				"     keys:['PartitionKey','id'],",
				"     skipKeyWrites:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          PartitionKey,",
				"          RowKey,",
				"          type,",
				"          tagNumber,",
				"          description,",
				"          facility,",
				"          area,",
				"          system,",
				"          discipline,",
				"          procurementPackage,",
				"          processLine,",
				"          id,",
				"          topTag,",
				"          attributes",
				"     )) ~> mysqlEquipmenrSink"
			]
		}
	}
}