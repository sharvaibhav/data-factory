{
	"name": "TestingDataFlow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "MYSQLEquipment",
						"type": "DatasetReference"
					},
					"name": "mysqlEquipment"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "MySqlEquipmentSink",
						"type": "DatasetReference"
					},
					"name": "mysqlEquipmenrSink"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          PartitionKey as string,",
				"          RowKey as string,",
				"          Timestamp as string,",
				"          type as string,",
				"          tagNumber as string,",
				"          description as string,",
				"          relations as string,",
				"          facility as string,",
				"          area as string,",
				"          system as string,",
				"          discipline as string,",
				"          procurementPackage as string,",
				"          processLine as string,",
				"          id as string,",
				"          topTag as string,",
				"          attributes as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     batchSize: 3,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> mysqlEquipment",
				"mysqlEquipment sink(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     input(",
				"          PartitionKey as string,",
				"          RowKey as string,",
				"          Timestamp as string,",
				"          type as string,",
				"          tagNumber as string,",
				"          description as string,",
				"          relations as string,",
				"          facility as string,",
				"          area as string,",
				"          system as string,",
				"          discipline as string,",
				"          procurementPackage as string,",
				"          processLine as string,",
				"          id as string,",
				"          topTag as string,",
				"          attributes as any",
				"     ),",
				"     deletable:false,",
				"     insertable:false,",
				"     updateable:false,",
				"     upsertable:true,",
				"     keys:['PartitionKey','id'],",
				"     skipKeyWrites:true,",
				"     format: 'table') ~> mysqlEquipmenrSink"
			]
		}
	}
}